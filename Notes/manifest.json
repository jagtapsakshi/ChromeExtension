{
    "manifest_version": 3,
    "name": "Quick Notes",
    "description": "A simple extension to take quick notes.",
    "version": "1.0",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icons16.png",
        "48": "icons/icons48.png",
        "128": "icons/icons128.png"
      }
    },
    "icons": {
      "16": "icons/icons16.png",
      "48": "icons/icons48.png",
      "128": "icons/icons128.png"
    }
  }
  